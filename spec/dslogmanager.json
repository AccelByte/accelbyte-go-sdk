  "paths": {
    "/dslogmanager/namespaces/{namespace}/servers/search": {
      "get": {
        "description": "```\nRequired permission: ADMIN:NAMESPACE:{namespace}:DSLM:SERVER [READ]\n\nThis endpoint used to retrieve terminated servers in a namespace\n```",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json",
          "text/x-log"
        ],
        "tags": [
          "Terminated Servers"
        ],
        "summary": "Retrieve All Terminated Servers",
        "operationId": "listTerminatedServers",
        "parameters": [
          {
            "type": "string",
            "description": "namespace of the game",
            "name": "namespace",
            "in": "path",
            "required": true
          },
          {
            "type": "string",
            "description": "region of the server",
            "name": "region",
            "in": "query"
          },
          {
            "type": "string",
            "description": "provider of the server",
            "name": "provider",
            "in": "query"
          },
          {
            "type": "string",
            "description": "session id of the game",
            "name": "session_id",
            "in": "query"
          },
          {
            "type": "string",
            "description": "pod name of the server",
            "name": "pod_name",
            "in": "query"
          },
          {
            "type": "string",
            "description": "ID of the party",
            "name": "party_id",
            "in": "query"
          },
          {
            "type": "string",
            "description": "ID of the user",
            "name": "user_id",
            "in": "query"
          },
          {
            "type": "string",
            "description": "game mode of the game",
            "name": "game_mode",
            "in": "query"
          },
          {
            "type": "string",
            "description": "deployment of the game",
            "name": "deployment",
            "in": "query"
          },
          {
            "type": "integer",
            "default": 0,
            "description": "offset",
            "name": "offset",
            "in": "query"
          },
          {
            "type": "integer",
            "default": 20,
            "description": "limit",
            "name": "limit",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/models.ListTerminatedServersResponse"
            }
          },
          "400": {
            "description": "Bad Request",
            "schema": {
              "$ref": "#/definitions/response.Error"
            }
          },
          "401": {
            "description": "Unauthorized",
            "schema": {
              "$ref": "#/definitions/response.Error"
            }
          },
          "500": {
            "description": "Internal Server Error",
            "schema": {
              "$ref": "#/definitions/response.Error"
            }
          }
        }
      }
    },
    "/dslogmanager/namespaces/{namespace}/servers/{podName}/logs/download": {
      "get": {
        "description": "Required permission: ADMIN:NAMESPACE:{namespace}:DSLM:LOG [READ]\n\nRequired scope: social\n\nThis endpoint will download dedicated server's log file (.log).",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json",
          "text/x-log"
        ],
        "tags": [
          "Download Server Logs"
        ],
        "summary": "Download dedicated server log files",
        "operationId": "downloadServerLogs",
        "parameters": [
          {
            "type": "string",
            "description": "namespace of the game",
            "name": "namespace",
            "in": "path",
            "required": true
          },
          {
            "type": "string",
            "description": "name of the DS pod",
            "name": "podName",
            "in": "path",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "server logs downloaded."
          },
          "404": {
            "description": "Not Found",
            "schema": {
              "$ref": "#/definitions/response.Error"
            }
          },
          "500": {
            "description": "Internal Server Error",
            "schema": {
              "$ref": "#/definitions/response.Error"
            }
          }
        }
      }
    },
    "/dslogmanager/namespaces/{namespace}/servers/{podName}/logs/exists": {
      "get": {
        "description": "Required permission: ADMIN:NAMESPACE:{namespace}:DSLM:LOG [READ]\n\nRequired scope: social\n\nThis endpoint will check log file existence before download file.",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json",
          "text/x-log"
        ],
        "tags": [
          "Download Server Logs"
        ],
        "summary": "Check dedicated server log files existence",
        "operationId": "checkServerLogs",
        "parameters": [
          {
            "type": "string",
            "description": "namespace of the game",
            "name": "namespace",
            "in": "path",
            "required": true
          },
          {
            "type": "string",
            "description": "name of the DS pod",
            "name": "podName",
            "in": "path",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Log exists.",
            "schema": {
              "$ref": "#/definitions/models.LogFileStatus"
            }
          },
          "404": {
            "description": "Not Found",
            "schema": {
              "$ref": "#/definitions/response.Error"
            }
          },
          "500": {
            "description": "Internal Server Error",
            "schema": {
              "$ref": "#/definitions/response.Error"
            }
          }
        }
      }
    },
